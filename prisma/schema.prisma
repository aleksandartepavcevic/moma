generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model bundle {
  id     Int    @id @default(autoincrement())
  name  String
  paid Int
  revenue Int @default(0)
  createdAt DateTime @default(now())
}
